{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "tailwind.config": [
        "./tailwind.config.js"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/screens/DashboardScreen.jsx",
    "src/screens/LoginScreen.jsx",
    "src/screens/RegisterScreen.jsx",
    "src/screens/PorfileScreen.jsx",
    "nativewind-env.d.ts"
  ]
}